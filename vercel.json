{
  "builds": [
    {
      "src": "next/package.json",
      "use": "@vercel/next"
    },
    {
      "src": "api/index.go",
      "use": "@vercel/go"
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index"
    },
    {
      "source": "/(.*)",
      "destination": "/"
    }
  ]
}
